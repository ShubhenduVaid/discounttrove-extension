{
  "name": "discounttrove",
  "description": "An extension to find cheapest products for Amazon",
  "version": "0.0.3",
  "manifest_version": 3,
  "action": {
    "default_title": "discounttrove",
    "default_popup": "discounttrove.html",
    "default_icon": "discounttrove.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "192": "icons/icon192.png",
    "512": "icons/icon512.png"
  },
  "permissions": ["activeTab", "scripting"]
}
